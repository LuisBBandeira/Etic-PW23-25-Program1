
funcao IMC():
    entrada("Introduz o teu peso:" , x)

    entrada("Introduz a tua altura:" , y)

    imc = x / (y*y)

    se imc <= 18,4
        imprime("Estas abaixo do peso")

    se 18,5 <= imc < 24.9
        imprime("O teu peso esta normal")

    se imc > 25
        imprime("Estas acima do peso") 